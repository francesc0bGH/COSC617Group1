    <header class="page-header header bjj container-fluid">
            <div class="overlay"></div>
            <div class="description">
                    <h1><%= name %>, welcome to BJJ Home Page!</h1>
                  </div>

                  
    </header>

    <div class="container features">
            <div class="row">
              <div class="col-lg-6">
                  <div class="blogContainer">
                        <a id="add" href="/editor">Add a blog</a>
                        <h1>BJJ Blog List</h1>
                        <ol id="bjjBlogs"></ol>
                        <div class="text-centered table-wrapper-scroll-y my-custom-scrollbar">
                            <% for (var i = 0; i < ename.length; i++) { %>
                            <% if(typeof ename[i].title !== 'undefined' ) { %>
              
                            <table id="t01" class="table table-sm">
                              <tr>
                                <th colspan="2"><%= ename[i].title %></th>
                              </tr>
                              <tr>
                                <td><label for="keywords">Activity</label></td>
                                <td><%= ename[i].keywords %></td>
                              </tr>
                              <tr>
                                <td><label for="description">Description</label></td>
                                <td><%= ename[i].description %></td>
                              </tr>
                            </table></br>
                            <% } else { %>
                            <% } %>
                            <% }; %>
                          </div>
                  </div>
              </div>
              <div class="col-lg-6"> 
                <div class="meetupContainer">
                    <a id="add" href="/meetup">Add a meetup</a>
                    <h1>BJJ Climbing Meetup List</h1>
                    <ol id = "bjjMeetup"></ol>
                    <div class="text-centered table-wrapper-scroll-y my-custom-scrollbar">
                      <% for (var i = 0; i < ename.length; i++) { %>
                      <% if(typeof ename[i].eventname !== 'undefined' ) { %>
                      <table id="t01" class="table table-sm">
                        <tr>
                          <th colspan="2"><%= ename[i].eventname %></th>
                        </tr>
                        <tr>
                          <td><label for="location">Location</label></td>
                          <td><%= ename[i].location %></td>
                        </tr>
                        <tr>
                          <td><label for="start">Start</label></td>
                          <td><%= ename[i].start %></td>
                        </tr>
                        <tr>
                          <td><label for="end">End</label></td>
                          <td><%= ename[i].end %></td>
                        </tr>
                      </table></br>
                      <% } %>
                      <% }; %>
                    </div>
              </div>  
              </div>
            </div> 
            <br><br><br><br>
          </div>
</body>

